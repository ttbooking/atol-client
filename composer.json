{
    "name": "ttbooking/atol-client",
    "description": "ATOL API v3/v4 client for php",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Pavel Agaletskiy",
            "email": "pavel.agaletskiy@lamoda.ru"
        },
        {
            "name": "Sergey Danilchenko",
            "email": "s.danilchenko@ttbooking.ru"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^7.2.5 || ^8.0",
        "jms/serializer": "^3.4",
        "symfony/validator": "^4.0 || ^5.0",
        "guzzlehttp/guzzle": "^6.3 || ^7.0",
        "doctrine/annotations": "^1.7",
        "doctrine/cache": "^1.6",
        "myclabs/php-enum": "^1.8"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5 || ^9.0",
        "friendsofphp/php-cs-fixer": "^2.11"
    },
    "autoload": {
        "psr-4": {
            "Lamoda\\AtolClient\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lamoda\\AtolClient\\Tests\\": "tests/"
        }
    }
}
